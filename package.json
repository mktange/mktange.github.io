{
  "private": true,
  "name": "mktange-site",
  "homepage": "http://mktange.com",
  "title": "tange",
  "subtitle": "Thoughts, types, tools, tips and tricks.",
  "email": "mktange@gmail.com",
  "twitter": "mktangeDK",
  "linkedin": "mktange",
  "github": "mktange",
  "disqus": "mktange",
  "googleAnalyticsUA": "UA-83618719-1",
  "repository": "https://github.com/mktange/mktange.github.io",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build"
  },
  "phenomic": {
    "CNAME": true,
    "open": false
  },
  "#babel": "webpack-(development|production) are useful for webpack 2, otherwise use development|production",
  "babel": {
    "env": {
      "test": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "development": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-react-optimize",
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "webpack-development": {
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "webpack-production": {
        "presets": [
          "babel-preset-react-optimize",
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      }
    }
  },
  "stylelint": {
    "extends": "./node_modules/phenomic/lib/stylelint-config-recommended/index.js",
    "rules": {
      "declaration-block-no-ignored-properties": null
    }
  },
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.11",
    "@types/react-dom": "^0.14.23",
    "@types/react-fa": "^4.1.1",
    "@types/react-ga": "^1.4.7",
    "@types/react-helmet": "0.0.28",
    "awesome-typescript-loader": "^3.0.7",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.13.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^2.0.0",
    "json-loader": "^0.5.2",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.20.3",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.0",
    "react-disqus-comments": "^0.5.2",
    "react-disqus-thread": "^0.4.0",
    "react-dom": "^15.4.0",
    "react-ga": "^2.1.2",
    "react-helmet": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta",
    "react-redux": "^4.0.0",
    "react-router": "^2.3.0",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.2.0",
    "typescript": "^2.3.0-dev.20170222",
    "url-loader": "^0.5.7",
    "warning": "^3.0.0",
    "webpack": "^1.11.0",
    "whatwg-fetch": "^0.11.0"
  }
}
